function cppcompile()
{
  filename=$1
  re="^\#include \""
  while read line
  do
    if [[ $line =~ $re ]]; then
      temp=${line:9}
      temp1=${temp#\"}
      temp2=${temp1%\.*\"}
      g++-10 -std=c++17 -c $temp2.cpp
    fi
  done < $filename.cpp
  g++-10 -std=c++17 -c $filename.cpp
  g++-10 -o $filename *.o
  ./$filename
  rm *.o
}